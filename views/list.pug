html
    head 
        title= title
        link(rel="stylesheet", href='/styles.css')
    body
        div(class="main-container")
            div(class="container-header")
                h1 User List
            table
                tr 
                    th User
                    th Name
                    th Email
                    th Age
                    th Action
                each user in users
                    tr
                        td= user.username
                        td= user.name
                        td= user.email
                        td= user.age
                        td
                            a(href='/edit/'+ user.userId)= 'Edit'
                            a(href='/delete/'+ user.userId)= 'Delete'
            div(class="btn-container")
                a.btn(href='/form') +



